    link(href="/libs/tagsinput/jquery.tagsinput.css", rel="stylesheet")
    link(href="/libs/plugins/jquery.plugins.css", rel="stylesheet")
    
    link(href="/admin/css/admin.css", type="text/css", rel="stylesheet")

    script(src="/libs/jquery.min.js")
