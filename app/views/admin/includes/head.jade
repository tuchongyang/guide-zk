    meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")
    meta(name="viewport", content="width=device-width,initial-scale=1")

    title=  title + ' - ' + pkg.name
    meta(name="keywords", content="{{ pkg.keywords.join(',') }}")
    meta(name="description", content="{{ pkg.description }}")

    meta(property="fb:app_id", content="293989217296609")
    meta(property="og:title", content=pkg.name + ' - ' + title)
    meta(property="og:description", content=pkg.description)
    meta(property="og:url", content="")
    meta(property="og:site_name", content="Node.js Express Mongoose movie site")
    meta(property="fb:admins", content="779311998")

    link(href="/libs/bootstrap/bootstrap.min.css", rel="stylesheet")
